{
  "plugins": [
    "babel-plugin-transform-object-rest-spread"
  ],
  "env": {
    "lib": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": [
              "last 5 versions",
              "defaults"
            ]
          },
          "include": ["transform-es2015-classes"]
        }]
      ]
    },
    "es": {
      "presets": [
        ["env", {
          "targets": {
            "chrome": 52
          },
          "loose": true,
          "modules": false
        }]
      ]
    }
  }
}
